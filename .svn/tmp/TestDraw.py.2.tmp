import curses
from Cube import Cube


def main(screen):
    import ThreeD.Graphics2D as g2d # Fix later
    try:
        curses.curs_set(0)
    except:
        pass
    curses.start_color()

    cube = Cube()
    
    g2d.draw_line(screen, (0,0), (10,5))
    screen.refresh()
    screen.getch()

if __name__ == '__main__':
    curses.wrapper(main)
